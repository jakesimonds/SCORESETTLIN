# PRIVATE KEY (for contract deployment - NEVER commit!)
PRIVATE_KEY=0x...your_private_key

# CELO
TAPBET_CONTRACT_ADDRESS=0x...fill_after_deployment

# BLUESKY (ATProto) - FREE!
# Get app password: Settings → Privacy and Security → App Passwords
BLUESKY_HANDLE=yourname.bsky.social
BLUESKY_APP_PASSWORD=xxxx-xxxx-xxxx-xxxx

# FILECOIN
FILECOIN_API_KEY=your_filecoin_key

# TEST MODE
TEST_MODE=true
